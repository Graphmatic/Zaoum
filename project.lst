                                           ;; --------------------------------------------------------------------------
                                           ;;  Hardware specific initialisation (see aout.inc for details)
                                           ;; --------------------------------------------------------------------------
                                           AOUT_InitHW
                                           	;; deactivate chip select
006ffc   8a8b     bsf	0x8b, 0x5, 0         	bsf	AOUT_LAT_CS, AOUT_PIN_CS
                                           
                                           	;; enable pin drivers
006ffe   9a94     bcf	0x94, 0x5, 0         	bcf	AOUT_TRIS_CS, AOUT_PIN_CS
007000   9894     bcf	0x94, 0x4, 0         	bcf	AOUT_TRIS_DIN, AOUT_PIN_DIN
007002   9a95     bcf	0x95, 0x5, 0         	bcf	AOUT_TRIS_SCLK, AOUT_PIN_SCLK
                                           
007004   0012     return	0                 	return
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  Sets the digital pins (see aout.inc for details)
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_DigitalPinsSet		; (for C)
                                           AOUT_DigitalPinsSet
                                           	;; temporary store gate flags in PRODL
007006   6ef3     movwf	0xf3, 0            	movwf	PRODL
                                           
                                           	;; commands to set the UPO:
                                           	;; UPO=low:  A1=0, A0=0, C1=1, C0=0
                                           	;; UPO=high: A1=0, A0=1, C1=1, C0=0
                                           
                                           	;; ensure that clock starts at low level
007008   9a8c     bcf	0x8c, 0x5, 0                 bcf	AOUT_LAT_SCLK, AOUT_PIN_SCLK
                                           
                                           	;; activate chip select
00700a   9a8b     bcf	0x8b, 0x5, 0         	bcf	AOUT_LAT_CS, AOUT_PIN_CS
                                           
                                           	;; update last MAX525 in the chain
00700c   6a03     clrf	0x3, 0              	clrf	MIOS_PARAMETER1
00700e   0e20     movlw	0x20               	movlw	0x20
007010   b2f3     btfsc	0xf3, 0x1, 0       	btfsc	PRODL, 1	; bit #1
007012   0940     iorlw	0x40               	iorlw	0x40
007014   6e04     movwf	0x4, 0             	movwf	MIOS_PARAMETER2
007016   d844     rcall	0x70a0             	rcall	AOUT_LoadWord
                                           
                                           	;; update first MAX525 in the chain
007018   6a03     clrf	0x3, 0              	clrf	MIOS_PARAMETER1
00701a   0e20     movlw	0x20               	movlw	0x20
00701c   b0f3     btfsc	0xf3, 0, 0         	btfsc	PRODL, 0	; bit #0
00701e   0940     iorlw	0x40               	iorlw	0x40
007020   6e04     movwf	0x4, 0             	movwf	MIOS_PARAMETER2
007022   d83e     rcall	0x70a0             	rcall	AOUT_LoadWord
                                           
                                           	;; deactivate chip select
007024   8a8b     bsf	0x8b, 0x5, 0         	bsf	AOUT_LAT_CS, AOUT_PIN_CS
                                           
007026   0012     return	0                 	return
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  Update function (see aout.inc for details)
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_Update			; (for C)
                                           AOUT_Update
                                           	;; skip if no update requested
                                           	SET_BSR	AOUT_UPDATE_REQ
00702a   5161     movf	0x61, 0, 0x1        	movf	AOUT_UPDATE_REQ, W, BANKED
00702c   e020     bz	0x706e                	bz	AOUT_Update_End
                                           
00702e   9a8c     bcf	0x8c, 0x5, 0                 bcf	AOUT_LAT_SCLK, AOUT_PIN_SCLK	; ensure that clock starts at low level
                                           
                                           	;; check which channels should be updated
007030   ee1f     lfsr	0x1, 0xf62          	lfsr	FSR1, AOUT_VALUES	; base pointer to 16bit value array
007032   f062
007034   6af3     clrf	0xf3, 0             	clrf	PRODL			; loop counter
007036   0e11     movlw	0x11               	movlw	0x11			; AND mask (running ones)
007038   6ef4     movwf	0xf4, 0            	movwf	PRODH
                                           AOUT_Update_Loop
                                           	;; check if one of the two channels has been updated
00703a   38f4     swapf	0xf4, 0, 0         	swapf	PRODH, W
00703c   10f4     iorwf	0xf4, 0, 0         	iorwf	PRODH, W
00703e   1561     andwf	0x61, 0, 0x1       	andwf	AOUT_UPDATE_REQ, W, BANKED
007040   e00e     bz	0x705e                	bz	AOUT_Update_Loop_Next
                                           
                                           	;; activate chip select
007042   9a8b     bcf	0x8b, 0x5, 0         	bcf	AOUT_LAT_CS, AOUT_PIN_CS
                                           
                                           	;; update last MAX525 in the chain
007044   0e08     movlw	0x8                	movlw	4*2		; switch to appr. array position
007046   26e1     addwf	0xe1, 0x1, 0       	addwf	FSR1L, F
007048   0e04     movlw	0x4                	movlw	4
00704a   26f3     addwf	0xf3, 0x1, 0       	addwf	PRODL, F
00704c   3af4     swapf	0xf4, 0x1, 0       	swapf	PRODH, F	; (running one now located at high-nibble)
00704e   d811     rcall	0x7072             	rcall	AOUT_Update_Hlp
                                           
                                           	;; update first MAX525 in the chain
007050   0ef8     movlw	0xf8               	movlw	-4*2		; switch back to initial array position
007052   26e1     addwf	0xe1, 0x1, 0       	addwf	FSR1L, F
007054   0efc     movlw	0xfc               	movlw	-4
007056   26f3     addwf	0xf3, 0x1, 0       	addwf	PRODL, F
007058   3af4     swapf	0xf4, 0x1, 0       	swapf	PRODH, F	; (running one now located at low-nibble)	
00705a   d80b     rcall	0x7072             	rcall	AOUT_Update_Hlp
                                           
00705c   8a8b     bsf	0x8b, 0x5, 0         	bsf	AOUT_LAT_CS, AOUT_PIN_CS	; deactivate chip select
                                           
                                           AOUT_Update_Loop_Next
                                           	;; increment loop counter, and exit if all 8 channels processed
00705e   2af3     incf	0xf3, 0x1, 0        	incf	PRODL, F
                                           	BRA_IFSET PRODL, 2, ACCESS, AOUT_Update_End	; (each iteration services 2 channels)
                                           	;; else increment pointer to AOUT_VALUESx and check next channel
007064   0e02     movlw	0x2                	movlw	2
007066   26e1     addwf	0xe1, 0x1, 0       	addwf	FSR1L, F			; (assumed that AOUT_VALUES is located within 256 byte range)
007068   90d8     bcf	0xd8, 0, 0           	clrc					; shiftleft running-one mask
00706a   36f4     rlcf	0xf4, 0x1, 0        	rlf	PRODH, F
                                           	rgoto	AOUT_Update_Loop
                                           
                                           AOUT_Update_End
                                           	;; clear update flags
00706e   6b61     clrf	0x61, 0x1           	clrf	AOUT_UPDATE_REQ, BANKED
                                           
007070   0012     return	0                 	return
                                           
                                           
                                           
                                           ;; help function used in update loop
                                           AOUT_Update_Hlp
                                           	;; commands to load and update the DAC register:
                                           	;; A1=channel number[1], A0=channel number[0], C1=1, C0=1
007072   38e6     swapf	0xe6, 0, 0         	swapf	POSTINC1, W		; convert 16bit to 12bit value -> MIOS_PARAMETER[12]
007074   0b0f     andlw	0xf                	andlw	0x0f
007076   6e03     movwf	0x3, 0             	movwf	MIOS_PARAMETER1
                                           
007078   38e7     swapf	0xe7, 0, 0         	swapf	INDF1, W
00707a   0bf0     andlw	0xf0               	andlw	0xf0
00707c   1203     iorwf	0x3, 0x1, 0        	iorwf	MIOS_PARAMETER1, F
                                           
00707e   38e5     swapf	0xe5, 0, 0         	swapf	POSTDEC1, W
007080   0b0f     andlw	0xf                	andlw	0x0f
007082   6e04     movwf	0x4, 0             	movwf	MIOS_PARAMETER2
                                           
                                           	;; invert value if inversion flag set
007084   cf60     movff	0xf60, 0xfe8       	movff	AOUT_INVERTED, WREG	; (note: AOUT_INVERTED could be located in another bank...)
007086   ffe8
007088   14f4     andwf	0xf4, 0, 0         	andwf	PRODH, W
00708a   e003     bz	0x7092                	bz	AOUT_Update_Hlp_NotInverted
                                           AOUT_Update_Hlp_Inverted
00708c   1e03     comf	0x3, 0x1, 0         	comf	MIOS_PARAMETER1, F
00708e   0e0f     movlw	0xf                	movlw	0x0f
007090   1a04     xorwf	0x4, 0x1, 0        	xorwf	MIOS_PARAMETER2, F
                                           AOUT_Update_Hlp_NotInverted
                                           
                                           	;; insert channel number (== loop counter)
007092   38f3     swapf	0xf3, 0, 0         	swapf	PRODL, W	; located at [7:6]
007094   34e8     rlcf	0xe8, 0, 0          	rlf	WREG, W
007096   34e8     rlcf	0xe8, 0, 0          	rlf	WREG, W
007098   0bc0     andlw	0xc0               	andlw	0xc0
00709a   0930     iorlw	0x30               	iorlw	0x30		; [5:4] = 3
00709c   1204     iorwf	0x4, 0x1, 0        	iorwf	MIOS_PARAMETER2, F
                                           
                                           	;; transfer word to chip
                                           	rgoto	AOUT_LoadWord
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  Internal function to load a 16bit value into MAX525
                                           ;;  IN: word in MIOS_PARAMETER[12]
                                           ;;  USES: MIOS_PARAMETER3 as temporary register
                                           ;; --------------------------------------------------------------------------
                                           AOUT_LoadWord
                                           	;; NOTE: you will notice that the instructions are sometimes arranged
                                           	;; in a special order to ensure proper output signals - for example:
                                           	;; between a rising and a falling SCLK edge there is at least one
                                           	;; other instruction to ensure that the high pulse of the SCLK
                                           	;; is longer than 100 nS
                                           
0070a0   0e10     movlw	0x10               	movlw	16				; init loop counter
0070a2   6e05     movwf	0x5, 0             	movwf	MIOS_PARAMETER3
                                           AOUT_LoadWord_Loop
0070a4   988b     bcf	0x8b, 0x4, 0         	bcf	AOUT_LAT_DIN, AOUT_PIN_DIN	; set DIN depending on current MSB
0070a6   be04     btfsc	0x4, 0x7, 0        	btfsc	MIOS_PARAMETER2, 7
0070a8   888b     bsf	0x8b, 0x4, 0         	bsf	AOUT_LAT_DIN, AOUT_PIN_DIN
0070aa   3603     rlcf	0x3, 0x1, 0         	rlf	MIOS_PARAMETER1, F		; start to shift the 16-bit value
0070ac   8a8c     bsf	0x8c, 0x5, 0         	bsf	AOUT_LAT_SCLK, AOUT_PIN_SCLK	; rising clock edge
0070ae   3604     rlcf	0x4, 0x1, 0         	rlf	MIOS_PARAMETER2, F		; second step for the 16-bit shift
0070b0   9a8c     bcf	0x8c, 0x5, 0         	bcf	AOUT_LAT_SCLK, AOUT_PIN_SCLK	; falling clock edge
0070b2   2e05     decfsz	0x5, 0x1, 0       	decfsz	MIOS_PARAMETER3, F		; loop 16 times
                                           	rgoto	AOUT_LoadWord_Loop
                                           
0070b6   0012     return	0                 	return
                                           
                                           #endif
                                           #if AOUT_INTERFACE_TYPE == 2
                                           #include "aout_74hc595.inc"
                                           #endif
                                           #if AOUT_INTERFACE_TYPE == 3
                                           #include "aout_tlv5630.inc"
                                           #endif
                                           
                                           #if AOUT_INTERFACE_TYPE < 0 || AOUT_INTERFACE_TYPE >= 4
                                             error "specified AOUT_INTERFACE_TYPE not supported by AOUT driver!!!"
                                           #endif
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_Init
                                           ;;  DESCRIPTION: This function initializes the connected AOUT module
                                           ;;  It should be called from USER_Init during startup
                                           ;;  OUT:  -
                                           ;;  USES: BSR, MIOS_PARAMETER[123] as temporary registers
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_Init			; (for C)
                                           AOUT_Init
                                           	;; clear inversion flags
                                           	SET_BSR	AOUT_INVERTED
0070ba   6b60     clrf	0x60, 0x1           	clrf	AOUT_INVERTED, BANKED
                                           
                                           	;; set all voltages to 0V
0070bc   6a03     clrf	0x3, 0              	clrf	MIOS_PARAMETER1	; using MIOS_PARAMETER1 as loop counter
0070be   ee1f     lfsr	0x1, 0xf62          	lfsr	FSR1, AOUT_VALUES
0070c0   f062
                                           AOUT_Init_Loop
0070c2   6ae6     clrf	0xe6, 0             	clrf	POSTINC1	; low-byte
0070c4   6ae6     clrf	0xe6, 0             	clrf	POSTINC1	; high-byte
0070c6   2a03     incf	0x3, 0x1, 0         	incf	MIOS_PARAMETER1, F
                                           	BRA_IFCLR MIOS_PARAMETER1, 3, ACCESS, AOUT_Init_Loop	; loop 8 times
                                           
                                           	;; init hardware and request AOUT update
0070cc   d801     rcall	0x70d0             	rcall	AOUT_ReInit
                                           
                                           	;; update channels and exit
                                           	rgoto	AOUT_Update
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_ReInit
                                           ;;  DESCRIPTION: This function re-initializes the AOUT module, and requests 
                                           ;;  an refresh of all output channels according to the stored values in AOUT_VALUES
                                           ;; 
                                           ;;  The update will take place once AOUT_Update is called (e.g. from USER_Tick)
                                           ;; 
                                           ;;  Background info: the AOUT_Pin*Set functions only request an update if the
                                           ;;  output value has been changed (comparison between new and previous value)
                                           ;;  This improves performance a lot, especially when AOUT values are written
                                           ;;  periodically from a timer interrupt (see MIDIbox SID and MIDIbox FM)
                                           ;; 
                                           ;;  However, sometimes it is useful to force a refresh, e.g. after a patch
                                           ;;  change, to ensure that all AOUT channels are up-to-date, because it allows
                                           ;;  to connect (or exchange) the AOUT module during runtime.
                                           ;;  IN:   -
                                           ;;  OUT:  -
                                           ;;  USES: BSR
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_ReInit		; (for C)
                                           AOUT_ReInit
                                           	;; call hardware specific initialisation
                                           	;; (function located in selected aout_*.inc)
                                           	;; especially required for MBHP_AOUT_NG module to initialize TLV5630 registers
0070d0   df95     rcall	0x6ffc             	rcall	AOUT_InitHW
                                           
                                           
                                           	;; request update of all channels
                                           	SET_BSR	AOUT_UPDATE_REQ
0070d4   6961     setf	0x61, 0x1           	setf	AOUT_UPDATE_REQ, BANKED
                                           
0070d6   0012     return	0                 	return
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_InvertedSet
                                           ;;  DESCRIPTION: This function sets the inversion flags for the 8 AOUT channels
                                           ;;  IN:   flags in WREG
                                           ;;  OUT:  -
                                           ;;  USES: BSR
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_InvertedSet		; (for C)
                                           AOUT_InvertedSet
                                           	SET_BSR	AOUT_INVERTED
0070da   6f60     movwf	0x60, 0x1          	movwf	AOUT_INVERTED, BANKED
0070dc   0012     return	0                 	return
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_InvertedGet
                                           ;;  DESCRIPTION: This function returns the inversion flags for the 8 AOUT channels
                                           ;;  IN:   -
                                           ;;  OUT:  flags in WREG
                                           ;;  USES: BSR
                                           ;; --------------------------------------------------------------------------
                                           _AOUT_InvertedGet		; (for C)
                                           AOUT_InvertedGet
                                           	SET_BSR	AOUT_INVERTED
0070e0   5160     movf	0x60, 0, 0x1        	movf	AOUT_INVERTED, W, BANKED
0070e2   0012     return	0                 	return
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_Pin16bitSet
                                           ;;  DESCRIPTION: This function sets an output channel to a given 16-bit value.
                                           ;;  The output value won't be transfered to the module immediately, but will 
                                           ;;  be buffered in AOUT_[LH] instead. By calling AOUT_Update the requested
                                           ;;  changes will take place.
                                           ;;  IN:   channel number in WREG, 16bit value in MIOS_PARAMETER[12]
                                           ;;  OUT:  -
                                           ;;  USES: FSR1, BSR, MIOS_PARAMETER3 as temporary register
                                           ;; --------------------------------------------------------------------------
                                           AOUT_Pin16bitSet
                                           	;; temporary store channel number in MIOS_PARAMETER3
0070e4   6e05     movwf	0x5, 0             	movwf	MIOS_PARAMETER3
                                           
                                           	;; calculate address to array
0070e6   ee1f     lfsr	0x1, 0xf62          	lfsr	FSR1, AOUT_VALUES
0070e8   f062
0070ea   34e8     rlcf	0xe8, 0, 0          	rlf	WREG, W
0070ec   0bfe     andlw	0xfe               	andlw	0xfe
0070ee   26e1     addwf	0xe1, 0x1, 0       	addwf	FSR1L, F
                                           
                                           	;; check if value has been changed
0070f0   50e7     movf	0xe7, 0, 0          	movf	INDF1, W
0070f2   1803     xorwf	0x3, 0, 0          	xorwf	MIOS_PARAMETER1, W
0070f4   e104     bnz	0x70fe               	bnz	AOUT_Pin16bitSet_Changed
0070f6   0e01     movlw	0x1                	movlw	1
0070f8   50e3     movf	0xe3, 0, 0          	movf	PLUSW1, W
0070fa   1804     xorwf	0x4, 0, 0          	xorwf	MIOS_PARAMETER2, W
0070fc   e009     bz	0x7110                	bz	AOUT_Pin16bitSet_NotChanged
                                           
                                           AOUT_Pin16bitSet_Changed	
                                           	;; transfer value to AOUT_[LH] array element
0070fe   c003     movff	0x3, 0xfe6         	movff	MIOS_PARAMETER1, POSTINC1
007100   ffe6
007102   c004     movff	0x4, 0xfe5         	movff	MIOS_PARAMETER2, POSTDEC1
007104   ffe5
                                           
                                           	;; notify that value has to be updated
007106   5005     movf	0x5, 0, 0           	movf	MIOS_PARAMETER3, W
007108   ecc6     call	0x2d8c, 0           	call	MIOS_HLP_GetBitORMask
00710a   f016
                                           	SET_BSR	AOUT_UPDATE_REQ
00710e   1361     iorwf	0x61, 0x1, 0x1     	iorwf	AOUT_UPDATE_REQ, F, BANKED
                                           
                                           AOUT_Pin16bitSet_NotChanged
                                           
007110   0012     return	0                 	return
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_Pin7bitSet
                                           ;;  DESCRIPTION: This function sets an output channel to a given 7-bit value.
                                           ;;  Since the AOUT driver supports up to 16bit resolution, the lower bits
                                           ;;  will be padded with zero
                                           ;;  The output value won't be transfered to the module immediately, but will 
                                           ;;  be buffered in AOUT_[LH] instead. By calling AOUT_Update the requested
                                           ;;  changes will take place.
                                           ;;  IN:   channel number in WREG, 7bit value in MIOS_PARAMETER1
                                           ;;  OUT:  -
                                           ;;  USES: FSR1, BSR, MIOS_PARAMETER[23] as temporary registers
                                           ;; --------------------------------------------------------------------------
                                           AOUT_Pin7bitSet
                                           	;; convert 7bit to 16bit value - LSBs padded with zeros
007112   c003     movff	0x3, 0x4           	movff	MIOS_PARAMETER1, MIOS_PARAMETER2
007114   f004
007116   90d8     bcf	0xd8, 0, 0           	clrc
007118   3604     rlcf	0x4, 0x1, 0         	rlf	MIOS_PARAMETER2, F
00711a   6a03     clrf	0x3, 0              	clrf	MIOS_PARAMETER1
                                           
                                           	;; change channel
                                           	rgoto	AOUT_Pin16bitSet
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_DigitalPinsSet
                                           ;;  DESCRIPTION: This function sets the digital pins of an AOUT module
                                           ;;  Currently only the MAX525 based MBHP_AOUT module supports two
                                           ;;  digital pins - they are addressed with WREG[0] and WREG[1]
                                           ;;  IN:   state of digital pins in WREG
                                           ;;  OUT:  -
                                           ;;  USES: FSR1, BSR, MIOS_PARAMETER3 as temporary register
                                           ;; --------------------------------------------------------------------------
                                           ;; (function located in selected aout_*.inc)
                                           
                                           
                                           ;; --------------------------------------------------------------------------
                                           ;;  FUNCTION: AOUT_Update
                                           ;;  DESCRIPTION: updates the output channels of the connected AOUT module
                                           ;;  Should be called, whenever changes have been requested via AOUT_Pin*Set
                                           ;;  OUT:  -
                                           ;;  USES: PROD[LH] and MIOS_PARAMETER[123] as temporary registers
                                           ;; --------------------------------------------------------------------------
                                           ;; (function located in selected aout_*.inc)
                                           
                                           
                                           
                                           
                                           
                                           ; ==========================================================================
                                           	;; C function wrappers
                                           
                                           ;_AOUT_Init	; (no parameters)
                                           
                                           ;_AOUT_ReInit 	; (no parameters)
                                           
                                           ;_AOUT_InvertedSet	; (direct parameter passing via WREG)
                                           
