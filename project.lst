                                           	global	_AOUT_InvertedGet
                                           	global	_AOUT_Pin16bitSet
                                           	global	_AOUT_Pin7bitSet
                                           	global	_AOUT_DigitalPinsSet
                                           	global	_AOUT_Update
                                           
                                           
